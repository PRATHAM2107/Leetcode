        int score = 0;
        for(int i=0; i<s.length()-1; i++){
            if(s[i]=='0') zeros++;
            else ones--;
            score = max(score, zeros + ones);
        }
        
        for(char c: s)
            if(c=='1') ones++;
       int ones = 0, zeros = 0;
    int maxScore(string s) {
        return score;
    }
};
"
